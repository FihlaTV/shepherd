{
  "family": "shepherd-docker",
  "containerDefinitions": [
    {
      "name": "shepherd-docker",
      "image": "673049184378.dkr.ecr.us-east-1.amazonaws.com/shepherd:latest",
      "cpu": 10,
      "memory": 500,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "entryPoint": [
        "/usr/sbin/apache2",
        "-D",
        "FOREGROUND"
      ],
      "essential": true
    }
  ]
}